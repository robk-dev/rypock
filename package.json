{
    "name": "rypock",
    "private": true,
    "scripts": {
        "prettier": "prettier --single-quote --write \"./src/**/*.{ts}\"",
        "prettier:check": "prettier --single-quote --list-different \"./src/**/*.{ts}\"",
        "publish": "npm run build && lerna publish",
        "update": "lerna run --parallel update",
        "build": "lerna run --parallel build",
        "test": "lerna run --parallel test",
        "tests:unit": "lerna run --parallel tests:unit",
        "tests:integration": "lerna run --parallel tests:integration",
        "tests:incr": "lerna run test:incr --concurrency 1 --stream --since master ",
        "bootstrap": "lerna bootstrap"
    },
    "engines": {
        "node": ">= 12.18.3"
    },
    "license": "MIT",
    "devDependencies": {
        "@commitlint/cli": "^8.3.5",
        "@types/bcryptjs": "^2.4.2",
        "@types/body-parser": "^1.17.1",
        "@types/chai": "^4.2.12",
        "@types/connect-redis": "0.0.11",
        "@types/cookie-parser": "^1.4.2",
        "@types/cors": "^2.8.6",
        "@types/csurf": "^1.9.36",
        "@types/express": "^4.17.1",
        "@types/express-session": "^1.15.14",
        "@types/hapi__joi": "^15.0.4",
        "@types/helmet": "0.0.44",
        "@types/hpp": "^0.2.1",
        "@types/lodash": "^4.14.138",
        "@types/mocha": "^8.0.3",
        "@types/mongodb": "^3.5.27",
        "@types/node": "^12.7.4",
        "@types/object-hash": "^1.3.0",
        "@types/supertest": "^2.0.8",
        "@types/uuid": "^3.4.5",
        "chai": "^4.2.0",
        "cross-env": "^7.0.2",
        "dotenv": "^8.2.0",
        "husky": "^4.3.0",
        "lerna": "^3.22.1",
        "lint-staged": "^10.3.0",
        "mocha": "^8.1.3",
        "monorepo-run": "^0.4.2",
        "nodemon": "^1.19.1",
        "nyc": "^15.1.0",
        "prettier": "^1.18.2",
        "pretty-quick": "^1.11.1",
        "rimraf": "^3.0.0",
        "supertest": "^4.0.2",
        "ts-node": "^9.0.0",
        "tslint": "^5.19.0",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "^3.6.2",
        "vsts-npm-auth": "^0.37.0"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lerna run --concurrency 1 --stream precommit"
        }
    },
    "dependencies": {
        "@rypock/az-blob": "file:libs\\az-blob",
        "@rypock/express-backend": "file:apps\\express-backend",
        "@rypock/nosql": "file:libs\\nosql",
        "@rypock/react-frontend": "file:apps\\react-frontend",
        "@rypock/redis": "file:libs\\redis",
        "@rypock/shared": "file:libs\\shared",
        "@rypock/utils": "file:libs\\utils",
        "@rypock/winston-logger": "file:libs\\winston-logger"
    }
}
