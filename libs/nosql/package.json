{
    "name": "@rypock/nosql",
    "version": "0.1.1",
    "description": "Mongo Repo Wrapper",
    "author": "robk <robk@robk.dev>",
    "homepage": "https://github.com/robk-dev/rypock#readme",
    "license": "MIT",
    "main": "lib/index.js",
    "directories": {
        "lib": "lib",
        "test": "__tests__"
    },
    "files": [
        "lib"
    ],
    "publishConfig": {
        "access": "public"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/robk-dev/rypock.git"
    },
    "scripts": {
        "test": "nyc mocha -r ts-node/register __tests__/**/*.spec.ts --timeout 20000 --exit",
        "build": "node_modules/.bin/my-build",
        "my-bulk-command": "node_modules/.bin/my-bulk-command",
        "precommit": "lint-staged"
    },
    "bugs": {
        "url": "https://github.com/robk-dev/rypock/issues"
    },
    "devDependencies": {
        "@rypock/toolchain": "~0.1.1",
        "chai": "~4.2.0",
        "mocha": "~8.1.3",
        "nyc": "~15.1.0",
        "typescript": "~4.0.3",
        "@types/mocha": "~8.0.3",
        "@types/node": "~14.11.2",
        "@types/chai": "~4.2.12",
        "ts-node": "~9.0.0",
        "@types/mongodb": "~3.5.27"
    },
    "dependencies": {
        "@rypock/shared": "~0.1.1",
        "mongodb": "^3.3.2"
    },
    "lint-staged": {
        "src/**/*.{js,jsx,ts,tsx}": [
            "prettier --ignore-path --single-quote --write"
        ],
        "src/**/*.{json}": [
            "prettier --ignore-path --parser json --write"
        ],
        "src/**/*.{html,md,yml}": [
            "prettier --ignore-path --single-quote --write"
        ]
    }
}
